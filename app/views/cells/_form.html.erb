<div class="row">
	<div class="col-lg-12">
		<div class="panel panel-default">
			

			<div class="panel-body">
				<div class="row">
					<div class="col-lg-12">
						<%= form_for @cell, :html => {:role => "form"} do |f| %>
						<div class="form-group">
							<%= f.label :name %>
								<%= f.text_field :name, {:class => "form-control"} %>	
						</div>
						<div class="form-group">
							<%= f.label :street %>
							<%= f.text_field :street, {:class => "form-control"} %>
							<!-- <p class="help-block">Example block-level help text here.</p> -->
							
						</div>
						<div class="form-group">
							<%= f.label :schedule %>
							
								<%= f.text_field :schedule, {:class => "form-control"} %>
						</div>
						<div class="form-group">
							<%= f.label :host %>
							
								<%= f.text_field :host, {:class => "form-control"} %>
						</div>
						<div class="form-group">
							<%= f.label :category %>
							<%= f.select(:category, ['CrianÃ§as', 'Adolescentes', 'Jovens', 'Jovens Solteiros', 'Casados'], {}, { :class => 'form-control' }) %>
						</div>
						<div class="form-group">
							<%= f.label :week_day %>
							
								<%= f.text_field :week_day, {:class => "form-control"} %>
						</div>

						<div class="form-group">
							
								<%= f.submit 'Enviar', :class => 'btn btn-default'  %>
						</div>
						<% end %>
						<hr/>
						<%= link_to "Voltar", :controller => "cells", :action => "index" %>


					</div>
				</div>
			</div>
		</div>
	</div>
</div>

